# ===== ClimateLens Example Configuration =====

# App
PORT=3000
NODE_ENV=development
CACHE_TTL_SECONDS=3600

# Database
MONGODB_URI="mongodb://127.0.0.1:27017/climate"
DB_NAME=climate
REDIS_URL="redis://localhost:6379"

# GSOD weather data
DATA_DIR=data/gsod
CHUNK_SIZE=1000

# Google Cloud / Vertex AI
GOOGLE_APPLICATION_CREDENTIALS=/path/to/your/service-account.json
GOOGLE_CLOUD_LOCATION=us-central1
GOOGLE_CLOUD_PROJECT=YOUR_PROJECT_ID
GOOGLE_GENAI_USE_VERTEXAI=false

VERTEX_EMBED_MODEL=gemini-embedding-001
VERTEX_LLM_MODEL=gemini-2.0-flash
VERTEX_PROJECT=YOUR_PROJECT
VERTEX_REGION=us-central1
VERTEX_MODEL=gemini-embedding-001

# Gemini (GenAI) API
GEMINI_API_KEY=
GEMINI_MODEL=gemini-2.0-flash

# Atlas (MongoDB) credentials
ATLAS_CLUSTER=YOUR_CLUSTER_NAME
ATLAS_PRIVATE_KEY=
ATLAS_PROJECT_ID=
ATLAS_PUBLIC_KEY=
