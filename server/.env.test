# ===== ClimateLens Test Configuration =====

# App
PORT=3000
NODE_ENV=test
CACHE_TTL_SECONDS=60

# Database (use a disposable/fake DB for tests)
MONGODB_URI="mongodb://127.0.0.1:27017/climate_test"
DB_NAME=climate_test
REDIS_URL="redis://localhost:6379"

# GSOD weather data
DATA_DIR=data/gsod
CHUNK_SIZE=1000

# Google Cloud / Vertex AI (mock or disabled)
GOOGLE_GENAI_USE_VERTEXAI=false
VERTEX_EMBED_MODEL=gemini-embedding-001
VERTEX_LLM_MODEL=gemini-2.0-flash

# Gemini (GenAI) API (mock or test key)
GEMINI_MODEL=gemini-2.0-flash
GEMINI_API_KEY=

# Atlas (MongoDB) credentials (mock or local)
ATLAS_CLUSTER=Climatelens-cluster
ATLAS_PRIVATE_KEY=
ATLAS_PROJECT_ID=
ATLAS_PUBLIC_KEY=
